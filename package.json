{
  "name": "pdvindecoder",
  "version": "0.1.2",
  "description": "pdico ",
  "files": [
    "lib",
    "example"
  ],
  "scripts": {
    "prestart": "npm run build",
    "start": "nodemon src/index.js --exec babel-node --presets es2015,stage-2",
    "pretest": "npm run build",
    "test": "npm run build && mocha --compilers js:babel-core/register",
